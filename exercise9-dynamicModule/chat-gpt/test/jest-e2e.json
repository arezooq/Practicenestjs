{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@arezooq/nest-chatgpt/(.*)": "<rootDir>/../libs/nest-chatgpt/src/$1",
    "@arezooq/nest-chatgpt": "<rootDir>/../libs/nest-chatgpt/src"
  }
}